{
    "version": 2,
    "builds": [{
            "src": "iot_backend/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [{
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "iot_backend/wsgi.py"
        }
    ]
}