{
    "version": 2,
    "builds": [{
        "src": "iot_backend/wsgi.py",
        "use": "@vercel/python"
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "iot_backend/wsgi.py"
    }],
    "build": {
        "functions": {
            "api/**/*.js": {
                "runtime": "python-3.9.*"
            }
        }
    }
}